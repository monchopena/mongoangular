<!doctype html>
<html lang="en" ng-app="myApp">
<head>
  <meta charset="utf-8">
  <title>My AngularJS App</title>
  <link rel="stylesheet" href="css/app.css"/>
</head>
<body>
  

<div ng-controller="MainCtrl">

<ul>
  <li ng-repeat="media in medias">
    <p>_id: {{ media._id }}</p>
    <p>Artist: {{ media.Artist }}</p>
    <p>Title: {{ media.Title }}</p>
    <p>Type: {{ media.Type }}</p>
    <div ng-repeat="tracklist in media.Tracklist" style="background-color:#333333; margin-top:15px; color:white">
    	<p>Track: {{ tracklist.Track }}</p>
    	<p>Title: {{ tracklist.Title }}</p>
    	<p>Length: {{ tracklist.Length }}</p>
    </div>
  </li>
</ul>

</div>
  
  
  <!-- In production use:
  <script src="//ajax.googleapis.com/ajax/libs/angularjs/x.x.x/angular.min.js"></script>
  -->
  <script src="lib/angular/angular.js"></script>
  <script src="lib/angular/angular-route.js"></script>
  <script src="js/app.js"></script>
  <script src="js/services.js"></script>
  <script src="js/controllers.js"></script>
  <script src="js/filters.js"></script>
  <script src="js/directives.js"></script>
</body>
</html>